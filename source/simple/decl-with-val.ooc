main: func {
    
    answer = 1729 : Int
    printf("%d is the smallest number expressible as the sum of two cubes in two different ways.\n", answer)
    
    answer2 : Int = 73
    printf("%d is the smallest number (besides 1) which is one less than twice its reverse.\n", answer2)
    
    answer3 := 68
    printf("%d is the lowest uninteresting number, which makes it very interesting.\n", answer3)
    
}
